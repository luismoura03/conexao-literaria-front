{
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.formatOnSave": false,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "editor.codeActionsOnSave": [
    "source.fixAll.eslint"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "vue"
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#7bc8fa",
    "activityBar.background": "#7bc8fa",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#f60999",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#7bc8fa",
    "statusBar.background": "#4ab4f8",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#19a0f6",
    "statusBarItem.remoteBackground": "#4ab4f8",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#4ab4f8",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#4ab4f899",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#4ab4f8"
}
